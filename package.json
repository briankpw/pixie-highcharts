{
  "name": "pixie-highcharts-angular",
  "version": "0.0.0-development",
  "description": "Pixie Highcharts is a wrapper of the highcharts and apply two-way data-binding of the option like series, tooltip, color, xAxis, yAxis, navigator, range selector, and etc. pixie-highcharts is breaking down the option into data, type, xAxis, colors, navigator, tooltips as Input and events as EventEmitter.",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4300",
    "build": "npm run packagr",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "cp": "cp README.md lib",
    "copy": "copy README.md lib",
    "publish": "npm publish lib/dist --access=public",
    "packagr": "ng-packagr -p lib/package.json",
    "pack": "npm i lib/dist && npm start",
    "package": "npm run packagr && npm run pack",
    "cover": "ng test --code-coverage",
    "travis-deploy-once": "travis-deploy-once --pro",
    "semantic-release": "semantic-release",
    "semantic-release-pre": "semantic-release pre",
    "semantic-release-post": "semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/briankpw/pixie-highcharts.git"
  },
  "bugs": {
    "url": "https://github.com/briankpw/pixie-highcharts/issues"
  },
  "readme": "README.md",
  "homepage": "https://github.com/briankpw/pixie-highcharts#readme",
  "keywords": [
    "pixie",
    "highcharts",
    "visualization",
    "chart",
    "dataset",
    "tool",
    "pixie-transformer",
    "angular2-highcharts",
    "angular"
  ],
  "author": "Brian Koh Ping Weng <briankpw@gmail.com>",
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "pkgRoot": "lib/dist"
  },
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "core-js": "^2.5.4",
    "highcharts": "^7.0.3",
    "highcharts-angular": "^2.4.0",
    "pixie-highcharts": "file:lib/dist",
    "pixie-transformer": "^1.1.5",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "underscore": "^1.9.1",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.3",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.7.1",
    "protractor": "~5.4.0",
    "semantic-release": "^15.13.3",
    "travis-deploy-once": "^5.0.11",
    "ts-node": "~7.0.0",
    "tsickle": "^0.34.3",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  }
}
