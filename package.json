{
  "name": "pixie-highcharts-angular",
  "version": "0.0.0-development",
  "description": "Pixie Highcharts is a wrapper of the highcharts and apply two-way data-binding of the option like series, tooltip, color, xAxis, yAxis, navigator, range selector, and etc. pixie-highcharts is breaking down the option into data, type, xAxis, colors, navigator, tooltips as Input and events as EventEmitter.",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4300",
    "build": "ng build pixie-highcharts",
    "cp": "cp README.md projects/pixie-highcharts",
    "copy": "copy README.md projects\\pixie-highcharts",
    "publish": "npm publish dist/pixie-highcharts --access=public",
    "pack": "npm i dist/pixie-highcharts && npm start",
    "package": "npm run build && npm run pack",
    "cover": "ng test --code-coverage",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/briankpw/pixie-highcharts.git"
  },
  "bugs": {
    "url": "https://github.com/briankpw/pixie-highcharts/issues"
  },
  "readme": "README.md",
  "homepage": "https://github.com/briankpw/pixie-highcharts#readme",
  "keywords": [
    "pixie",
    "highcharts",
    "visualization",
    "chart",
    "dataset",
    "tool",
    "pixie-transformer",
    "angular2-highcharts",
    "angular",
    "angular8"
  ],
  "author": "Brian Koh Ping Weng <briankpw@gmail.com>",
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com/"
  },
  "release": {
    "pkgRoot": "dist/pixie-highcharts"
  },
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "highcharts": "^8.1.0",
    "highcharts-angular": "^2.4.0",
    "rxjs": "~6.4.0",
    "rxjs-compat": "^6.5.5",
    "tslib": "^1.10.0",
    "underscore": "^1.10.2",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.25",
    "@angular-devkit/build-ng-packagr": "~0.803.25",
    "@angular/cli": "~8.3.26",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^5.4.0",
    "protractor": "~5.4.0",
    "semantic-release": "^17.0.8",
    "ts-node": "~7.0.0",
    "tsickle": "^0.37.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  }
}
